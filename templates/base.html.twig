<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
        <link rel="stylesheet" href="/bootstrap/css/bootstrap.css">
        <link rel="stylesheet" href="/main.css">
        {% endblock %}
    </head>
    <body>
        <header class="container">
            <div class="row">
                <div class="col">
                    {{ render(controller(
                        'App\\Controller\\DefaultController::getBranding'
                    )) }}
                </div>
            </div>
        </header>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
